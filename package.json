{
  "name": "component-replacer",
  "displayName": "Component Replacer",
  "description": "Replace components in React files",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BestDingSheng/vscode-plugins-demo.git"
  },
  "activationEvents": [
    "onCommand:component-replacer.replaceComponents"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "component-replacer.replaceComponents",
        "title": "Replace Components"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "when": "editorFocus",
          "command": "component-replacer.replaceComponents",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "npm version patch && vsce package"
    },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.6",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.10.4",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@babel/generator": "^7.26.2",
    "@babel/parser": "^7.26.2",
    "@babel/traverse": "^7.25.9",
    "@babel/types": "^7.26.0",
    "prettier": "^3.4.0"
  }
}
